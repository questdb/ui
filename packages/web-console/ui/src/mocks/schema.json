[
  {
    "name": "trips",
    "description": "trips information for yellow and green cabs between 2009 and 2019",
    "columns": [
      {
        "name": "originating_file",
        "type": "symbol",
        "description": "csv file where the trip originates from"
      },
      {
        "name": "cab_type",
        "type": "symbol",
        "description": "cab type ('yellow' or 'green')"
      },
      {
        "name": "vendor_id",
        "type": "symbol",
        "description": "transportation vendor code ('VTC', 'CMT', 'DDS', 'OTHER')"
      },
      {
        "name": "pickup_datetime",
        "type": "timestamp",
        "description": "ride start timestamp"
      },
      {
        "name": "dropoff_datetime",
        "type": "timestamp",
        "description": "ride end timestamp"
      },
      {
        "name": "rate_code_id",
        "type": "symbol",
        "description": "special fare for selected rides ()"
      },
      {
        "name": "pickup_longitude",
        "type": "double",
        "description": "ride start longitude coordinate"
      },
      {
        "name": "pickup_latitude",
        "type": "double",
        "description": "ride start latitude coordinate"
      },
      {
        "name": "dropoff_longitude",
        "type": "double",
        "description": "ride end longitude coordinate"
      },
      {
        "name": "dropoff_latitude",
        "type": "double",
        "description": "ride end latitude coordinate"
      },
      {
        "name": "passenger_count",
        "type": "int",
        "description": "number of passengers"
      },
      {
        "name": "trip_distance",
        "type": "double",
        "description": "ride distance in miles"
      },
      {
        "name": "fare_amount",
        "type": "double",
        "description": "amount calculated by the meter"
      },
      {
        "name": "extra",
        "type": "double",
        "description": "extra from luggage or other items"
      },
      {
        "name": "mta_tax",
        "type": "double",
        "description": "metropolitan transit authority tax"
      },
      {
        "name": "tip_amount",
        "type": "double",
        "description": "tip amount"
      },
      {
        "name": "tolls_amount",
        "type": "double",
        "description": "amount to pay for tolls"
      },
      {
        "name": "ehail_fee",
        "type": "double",
        "description": "fee for hailing with an app"
      },
      {
        "name": "improvement_surcharge",
        "type": "double",
        "description": "surcharge introduced to finance accessible taxis"
      },
      {
        "name": "congestion_surcharge",
        "type": "double",
        "description": "fee for driving through the congestion zone"
      },
      {
        "name": "total_amount",
        "type": "double",
        "description": "total amount to pay inclusive of all fees"
      },
      {
        "name": "payment_type",
        "type": "symbol",
        "description": "means of payment used ('Cash' or 'Card', 'Voided', 'Dispute', 'Unknown', 'No Charge')"
      },
      {
        "name": "trip_type",
        "type": "symbol",
        "description": "how the ride was initiated ('Street hail', 'Dispatch', 'na')"
      },
      {
        "name": "pickup_location_id",
        "type": "int",
        "description": "ID of the pickup zone"
      },
      {
        "name": "dropoff_location_id",
        "type": "int",
        "description": "ID of the dropoff zone"
      }
    ]
  },

  {
    "name": "weather",
    "description": "hourly weather data from 2010 in Central Park",
    "columns": [
      {
        "name": "sS",
        "type": "timestamp",
        "description": "observation timestamp"
      },
      {
        "name": "sindDir",
        "type": "int",
        "description": "wind direction in degrees"
      },
      { "name": "sindSpeed", "type": "int", "description": "wind speed mph" },
      { "name": "sindGust", "type": "int", "description": "wind gust mph" },
      { "name": "sloudCeiling", "type": "int", "description": "cloud ceiling" },
      {
        "name": "skyCover",
        "type": "symbol",
        "description": "sky condition ('OVC', 'BKN', 'SCT', 'CLR', 'OBS')"
      },
      {
        "name": "sisibilityMiles",
        "type": "double",
        "description": "visibility condition in miles"
      },
      {
        "name": "sempF",
        "type": "int",
        "description": "temperature in Farenheit"
      },
      {
        "name": "sewpF",
        "type": "int",
        "description": "dew point in Farenheit"
      },
      {
        "name": "srecip_1h",
        "type": "double",
        "description": "inches of rain last hour"
      },
      {
        "name": "srecip_6h",
        "type": "double",
        "description": "inches of rain last 6 hours"
      },
      {
        "name": "srecip_24h",
        "type": "double",
        "description": "inches of rain last 24 hours"
      },
      {
        "name": "snowDepthInch",
        "type": "double",
        "description": "inches of snow current"
      }
    ]
  },

  {
    "name": "gas_price",
    "description": "weekly gas prices in NYC from 2000 to 2020",
    "columns": [
      {
        "name": "dt",
        "type": "timestamp",
        "description": "observation timestamp"
      },
      {
        "name": "galon_price",
        "type": "double",
        "description": "price per gallon of gas in NYC, USD"
      }
    ]
  },

  {
    "name": "fhv_trips",
    "description": "trips from for-hire vehicle services (uber, lyft etc)",
    "columns": [
      {
        "name": "originating_file",
        "type": "symbol",
        "description": "csv file where the trip originates from"
      },
      {
        "name": "dispatching_base_num",
        "type": "symbol",
        "description": "identification code of the dispatching entity. Mapping to be found in table fhv_bases"
      },
      {
        "name": "pickup_datetime",
        "type": "timestamp",
        "description": "ride start timestamp"
      },
      {
        "name": "dropoff_datetime",
        "type": "timestamp",
        "description": "ride end timestamp"
      },
      {
        "name": "pickup_location_id",
        "type": "int",
        "description": "ID of the pickup zone"
      },
      {
        "name": "dropoff_location_id",
        "type": "int",
        "description": "ID of the dropoff zone"
      },
      {
        "name": "shard_ride",
        "type": "boolean",
        "description": "flag set to true if the ride was a shared ride"
      },
      {
        "name": "hvfhs_license_number",
        "type": "symbol",
        "description": "license number for high-volume for-hire-services ('HV0002' Juno, 'HV0005' Lyft, 'HV0003' Uber)"
      }
    ]
  },
  {
    "name": "fhv_bases",
    "description": "mapping table for 'dispatching_base_num' in table fhv_trips",
    "columns": [
      {
        "name": "base_number",
        "type": "string",
        "description": "fhv base number"
      },
      {
        "name": "base_name",
        "type": "string",
        "description": "base corporation name"
      },
      {
        "name": "dba",
        "type": "string",
        "description": "main entity name"
      }
    ]
  }
]
